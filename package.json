        "laravel-mix": "^6.0.18",
        "lodash": "^4.17.13",
        "popper.js": "^1.12",
        "postcss": "^8.2.12",
        "resolve-url-loader": "^3.1.0",
        "sass": "^1.32.11",
        "sass-loader": "^8.0.0"
    },
